.selectTop{}
.selectTop .image{width: 74px; height: 74px; border-radius: 50%; margin-right: 10px}
.reportActive{ margin-top: 5px;}
.reportBtn{ height: 26px; line-height: 26px; border: 1px solid #000; border-radius: 2px; display: inline-block; width: 54%;text-align: center;}
.reportFollow{border: 1px solid #000; width: calc(46% - 100px); float:right;height: 26px; line-height: 26px;border-radius: 2px; vertical-align: middle}

.selectCenter{ margin-top: 10px}
.selectCenter .name{ line-height: 35px}

.selectBottom{ height: 30px; line-height: 30px; margin-top: 12px}
.selectBottom .pull-left{ margin-right: 10px}
.fansList{}
.fansList .fansImg{ width: 28px; height: 28px; border-radius: 50%; float: left; margin-right: 5px}


.scroll-x {display: flex;flex-direction: row;}
.liveSetion{}
.liveSetion .scroll-view{}
.liveSetion .scroll-view-item{padding:0 10px;display: inline-block}
.liveSetion .liveHeader{ width: 90px; height: 90px; margin: 0 auto; margin-bottom: 5px; border-radius: 50%;}

.swiperImage{ position: relative}
.swiperImage .time{ position: absolute; right: 10px; bottom: 5px; color: #fff; font-size: 14px}
.swiperImage .shareIcon{ top: 0;}

.selectTabTitle{ height: 40px; line-height: 40px}
.selectTabTitle .tabLine{ position: relative; font-size: 14px}
.selectTabTitle .tabLine:not(:last-child)::after{ content: ''; position: absolute; right: 0; background: #d8d8d8; width: 1px; height: 20px; top: 10px;}


.PictureList.rowList .imageList{ width: 33%; float: left; border-left: 1px solid #fff;border-bottom: 1px solid #fff}
.PictureList.rowList .imageList image{ width: 100%; height:125px; }
.PictureList.lineList{ width: 100%;}
.PictureList.lineList .imageList image{ height: 200px; width: 100%}











